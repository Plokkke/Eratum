{
	"name": "eratum",
	"version": "2.0.0",
	"author": "Antoine Caron <acaron@trustdesigner.com>",
	"license": "MIT",
	"description": "Module defining wrapper over Error class.",
	"keywords": [
		"Errors",
		"Throws",
		"Rejections",
		"Failures"
	],
	"repository": {
		"type": "git",
		"url": "git://gitlab.com:antoinecaron/eratum.git"
	},
	"main": "sources/index.ts",
	"scripts": {
		"test": "mocha -r ts-node/register **/*.test.ts **/*.test.js",
		"coverage": "nyc npm run test"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"sources"
		],
		"exclude": [
			"**/*.test.ts/",
			"sources/Validator.ts"
		],
		"reporter": [
			"text-summary",
			"html"
		]
	},
	"dependencies": {
		"@types/ejs": "^2.6.3",
		"@types/lodash": "^4.14.147",
		"@types/node": "^12.12.7",
		"ejs": "^2.7.2",
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/chai": "^4.2.5",
		"@types/mocha": "^5.2.7",
		"chai": "^4.2.0",
		"eslint": "^6.6.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-import": "^2.18.2",
		"mocha": "^4.1.0",
		"nyc": "^11.3.0",
		"ts-mocha": "^6.0.0",
		"ts-node": "^8.5.0",
		"typescript": "^3.7.2"
	}
}